{
  "name": "frontend-demo",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "typecheck": "tsc --noEmit",
    "build:vercel": "cd ../.. && pnpm --filter frontend-demo build",
    "vercel-build": "cd ../.. && npm install -g pnpm && pnpm install --frozen-lockfile && pnpm --filter ui build && cd apps/frontend-demo && next build"
  },
  "dependencies": {
    "next": "15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ui": "file:../../libs/ui"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.6",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "postcss": "^8",
    "tailwindcss": "^4.0.0-beta.8",
    "typescript": "^5"
  }
}
