{
  "name": "medusa-be",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "start": "medusa start",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "migrate": "medusa db:migrate",
    "syncLinks": "medusa db:sync-links",
    "createInitialSuperadmin": "medusa user -e \"$SUPERADMIN_EMAIL\" -p \"$SUPERADMIN_PASSWORD\"",
    "seedInitialData": "medusa exec ./src/scripts/seed.ts",
    "seedInitialDataTs": "medusa exec ./src/scripts/seed.ts",
    "seedDefaultData": "medusa exec ./src/scripts/seed-products-from-db.ts",
    "seedDefaultDataTs": "medusa exec ./src/scripts/seed-products-from-db.ts",
    "addInitialSearchDocuments": "medusa exec ./src/scripts/search-index.ts",
    "setInitialPublishableKey": "bash -c \"medusa exec ./src/scripts/output-publishable-key.ts | grep -o '<PK_TOKEN>.*</PK_TOKEN>' | sed 's/<PK_TOKEN>//g;s/<\\/PK_TOKEN>//g' | tr -d '\\n\\r' | zsc setSecretEnv CHANNEL_PUBLISHABLE_KEY\""
  },
  "dependencies": {
    "@medusajs/admin-sdk": "^2.7.0",
    "@medusajs/cli": "^2.7.0",
    "@medusajs/dashboard": "^2.7.0",
    "@medusajs/event-bus-redis": "^2.7.0",
    "@medusajs/framework": "^2.7.0",
    "@medusajs/medusa": "^2.7.0",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@neondatabase/serverless": "^0.10.4",
    "@rokmohar/medusa-plugin-meilisearch": "^1.0.6",
    "awilix": "^8.0.1",
    "drizzle-orm": "^0.40.0",
    "mime": "1.6.0",
    "pg": "^8.13.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@medusajs/test-utils": "^2.7.0",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.37",
    "@types/jest": "^29.5.13",
    "@types/mime": "1.3.5",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=22"
  }
}
