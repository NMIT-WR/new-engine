{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./.medusa/server",
    "rootDir": "./",
    "baseUrl": ".",
    "jsx": "react-jsx",
    "verbatimModuleSyntax": false,
    "emitDeclarationOnly": false,
    "declaration": false,
    "declarationMap": false,
    "composite": false,
    "incremental": true,
    "tsBuildInfoFile": "./.medusa/tsconfig.tsbuildinfo"
  },
  "ts-node": {
    "swc": true
  },
  "include": ["**/*", ".medusa/types/*"],
  "exclude": ["node_modules", ".medusa/*", ".cache"]
}
