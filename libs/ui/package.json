{
  "name": "@new-engine/ui",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.js"
    },
    "./tokens": {
      "import": "./src/tokens/tokens-only.css"
    },
    "./tokens-with-tailwind": {
      "import": "./src/tokens/index.css"
    },
    "./tokens/*": {
      "import": "./src/tokens/*.css"
    },
    "./atoms/*": {
      "types": "./dist/src/atoms/*.d.ts",
      "import": "./dist/atoms/*.js"
    },
    "./molecules/*": {
      "types": "./dist/src/molecules/*.d.ts",
      "import": "./dist/molecules/*.js"
    },
    "./utils": {
      "types": "./dist/src/utils.d.ts",
      "import": "./dist/utils.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src/tokens"
  ],
  "scripts": {
    "build": "rslib build",
    "build:storybook": "storybook build",
    "dev": "rslib build --watch",
    "storybook": "storybook dev",
    "lint:tailwind": "eslint \"**/*.{js,jsx,ts,tsx}\" --config eslint.config.js",
    "lint:tailwind:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --config eslint.config.js --fix",
    "check:unused-tokens": "node scripts/check-unused-tokens.js",
    "validate:tokens": "node scripts/validate-token-usage.js && node scripts/validate-token-definitions.js",
    "validate:token-usage": "node scripts/validate-token-usage.js",
    "validate:token-definitions": "node scripts/validate-token-definitions.js"
  },
  "dependencies": {
    "@zag-js/accordion": "^1.15.0",
    "@zag-js/carousel": "^1.15.0",
    "@zag-js/checkbox": "^1.15.0",
    "@zag-js/combobox": "^1.15.0",
    "@zag-js/dialog": "^1.15.0",
    "@zag-js/i18n-utils": "^1.15.0",
    "@zag-js/menu": "^1.15.0",
    "@zag-js/number-input": "^1.15.0",
    "@zag-js/pagination": "^1.15.0",
    "@zag-js/popover": "^1.15.0",
    "@zag-js/rating-group": "^1.15.0",
    "@zag-js/react": "^1.15.0",
    "@zag-js/select": "^1.15.0",
    "@zag-js/slider": "^1.15.0",
    "@zag-js/steps": "^1.15.0",
    "@zag-js/switch": "^1.15.0",
    "@zag-js/tabs": "^1.15.0",
    "@zag-js/toast": "^1.15.0",
    "@zag-js/tooltip": "^1.15.0",
    "@zag-js/tree-view": "^1.15.0",
    "tailwind-variants": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@iconify/json": "^2.2.338",
    "@iconify/tailwind4": "^1.0.6",
    "@rsbuild/core": "1.3.5",
    "@rsbuild/plugin-react": "^1.3.1",
    "@rslib/core": "^0.6.9",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/addon-themes": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@tailwindcss/postcss": "^4.1.6",
    "@types/react": "^19.1.4",
    "@typescript-eslint/parser": "^8.33.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.0.0",
    "eslint-plugin-tailwindcss": "npm:@hyoban/eslint-plugin-tailwindcss@4.0.0-alpha.12",
    "glob": "^11.0.2",
    "next": "^15.5.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "storybook": "^8.6.12",
    "storybook-addon-rslib": "^1.0.1",
    "storybook-react-rsbuild": "^1.0.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.6",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": ">=19.1.0",
    "react-dom": ">=19.1.0"
  }
}
