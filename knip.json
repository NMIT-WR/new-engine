{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "paths": {
    "@libs/data-layer": ["./libs/data-layer/src"],
    "@libs/data-layer/*": ["./libs/data-layer/src/*"],
    "@libs/ui": ["./libs/ui/src"],
    "@libs/ui/*": ["./libs/ui/src/*"],
    "@libs/analytics": ["./libs/analytics/src"],
    "@libs/analytics/*": ["./libs/analytics/src/*"],
    "@libs/medusa-sdk": ["./libs/medusa-sdk/src"],
    "@libs/medusa-sdk/*": ["./libs/medusa-sdk/src/*"],
    "@libs/product-hooks": ["./libs/product-hooks/src"],
    "@libs/product-hooks/*": ["./libs/product-hooks/src/*"]
  },
  "ignoreBinaries": ["build", "semantic-release"],
  "ignoreDependencies": [
    "@iconify/json",
    "@iconify/tailwind4",
    "tslib"
  ],
  "ignoreUnresolved": [
    "^\\.\\./\\*\\*/\\*\\.\\{js,ts,jsx,tsx,mdx\\}$"
  ],
  "ignore": [
    "**/dist/**",
    "**/.next/**",
    "**/.medusa/**",
    "**/build/**",
    "**/storybook-static/**",
    "**/coverage/**",
    "**/.netlify/**",
    "**/e2e/**",
    "**/playwright-report/**",
    "**/*.generated.{ts,tsx}",
    "**/.cache/**",
    "**/local/**",
    "reports/**"
  ],
  "ignoreExportsUsedInFile": {
    "interface": true,
    "type": true
  },
  "ignoreIssues": {
    "**/*.stories.tsx": ["exports"],
    "**/*.stories.ts": ["exports"],
    "**/tokens/**/*.ts": ["exports", "types"],
    "**/tokens/**/*.css": ["files"]
  },
  "workspaces": {
    "apps/medusa-be": {
      "entry": [
        "src/api/**/*.{ts,tsx}!",
        "src/modules/**/*.{ts,tsx}!",
        "src/workflows/**/*.{ts,tsx}!",
        "src/jobs/**/*.{ts,tsx}!",
        "src/admin/**/*.{ts,tsx}!",
        "src/scripts/**/*.{ts,tsx}!",
        "tests/**/*.{ts,tsx}",
        "integration-tests/**/*.{js,ts}",
        "medusa-config.ts!"
      ],
      "ignoreDependencies": [
        "@medusajs/admin-shared",
        "@medusajs/caching-redis",
        "@medusajs/dashboard",
        "@medusajs/draft-order",
        "@medusajs/event-bus-redis",
        "@medusajs/index"
      ],
      "ignore": [
        ".medusa/**",
        "dist/**"
      ]
    },
    "apps/frontend-demo": {
      "entry": [
        "src/app/**/*.{tsx,ts}!",
        "next.config.{js,ts,mjs}!"
      ],
      "ignoreDependencies": [
        "tailwindcss-animate"
      ],
      "ignore": [
        ".next/**",
        ".netlify/**"
      ]
    },
    "apps/n1": {
      "entry": [
        "src/app/**/*.{tsx,ts}!",
        "next.config.{js,ts,mjs}!"
      ],
      "ignore": [
        ".next/**",
        ".netlify/**"
      ]
    },
    "apps/herbatika": {
      "entry": [
        "src/**/*.{ts,tsx}!"
      ]
    },
    "libs/ui": {
      "entry": [
        "src/atoms/*.tsx!",
        "src/molecules/*.tsx!",
        "src/organisms/*.tsx!",
        "src/templates/*.tsx!",
        "src/types/*.ts!",
        "src/utils.ts!",
        "release.config.mjs"
      ],
      "project": [
        "src/**/*.{ts,tsx}"
      ],
      "storybook": {
        "config": [".storybook/main.ts"],
        "entry": [
          "**/*.stories.{ts,tsx}",
          ".storybook/**/*.{ts,tsx}"
        ]
      },
      "ignoreBinaries": [
        "eslint"
      ],
      "ignoreDependencies": [
        "^@semantic-release/"
      ],
      "ignore": [
        "storybook-static/**",
        "local/**"
      ]
    },
    "libs/analytics": {
      "entry": [
        "src/index.ts!",
        "src/core/index.ts!",
        "src/meta/index.ts!",
        "src/google/index.ts!",
        "src/heureka/index.ts!",
        "src/leadhub/index.ts!"
      ],
      "ignoreDependencies": [
        "@types/react-dom"
      ]
    },
    "libs/data-layer": {
      "entry": [
        "src/index.ts!"
      ]
    },
    "libs/medusa-sdk": {
      "entry": [
        "src/index.ts!"
      ]
    },
    "libs/product-hooks": {
      "entry": [
        "src/index.ts!"
      ]
    }
  },
  "rules": {
    "files": "warn",
    "dependencies": "warn",
    "devDependencies": "warn",
    "unlisted": "error",
    "exports": "warn",
    "types": "warn",
    "duplicates": "warn"
  }
}
